<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1000.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sapper.scoreBoard.ScoreBoard">
   <children>
      <TableView fx:id="recordTable" layoutX="4.0" layoutY="4.0" prefHeight="956.0" prefWidth="1491.0">
        <columns>
          <TableColumn fx:id="playerCol" minWidth="0.0" prefWidth="617.0" text="Player Name" />
          <TableColumn fx:id="levelCol" minWidth="0.0" prefWidth="229.0" text="Level" />
            <TableColumn fx:id="timeCol" prefWidth="644.0" text="Time" />
        </columns>
      </TableView>
      <Button fx:id="returnBtn" layoutX="563.0" layoutY="960.0" mnemonicParsing="false" onKeyPressed="#returnToMenu" prefHeight="38.0" prefWidth="386.0" text="Return">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
